<% ua = Hash[[ 'ie?', 'capable?', 'meta' ].zip([ browser.ie?, browser.capable?, browser.meta ])] %>
<div id="main">

  <div id="north">
    <% if ( ua['ie?'] && /(\d+)$/.match(ua['meta'][1]).to_s.to_i < 9 ) %> <%# !(ua['capable?']) ||  %>
      Nous vous conseillons de mettre votre navigateur à jour ou d'utiliser Firefox, Chrome, Safari, Opera, etc.
    <% end %>
  </div>

  <div id="content">

    <% if ezinottub("buttons/redCircle210", 170, 170, "/library/Fonts/SerifaStd-Black.otf", 15, "FFFFFF", "Center", "retour sur\nles rencontres\n2013", "aHeaderH1") %>
	  <%= link_to("", bienvenue_path, id: "aHeaderH1") %>
	<% end %>
    <%# if ezinottub("buttons/redCircle150", 112, 80, "/library/Fonts/SerifaStd-Bold.otf", 12, "FFFFFF", "Center", "accueil", "divHeaderH1") %>
      <!-- <div id="divHeaderH1"></div> -->
    <%# end %>
    <% if eziser(140, "buttons/fondation456", "tif", "podium/fondation", "jpg") %>
      <%= link_to("", "http://www.fondation.univ-montp1.fr/", id: "aContentSouth", target: "_blank") %>
     <% end %>
    <%= eziser(150, "clap-oscar-100", "png", "ziyKaJweSm046", "png") ? link_to("", "http://www.preventionbtp.fr/Magazine/La-prevention-autrement/50-ans-de-prevention", id: "yizKaJweSm046", target: "_blank") : nil %>
	<!-- // overlays -->

    <div class="yui3-g" id="header">
      <div class="yui3-u-1-5"><h1></h1></div>
      <div class="yui3-u-4-5" id="contact">
        contact : <a href="mailto:fondation@univ-montp1.fr">fondation@univ-montp1.fr</a>
      </div>
    </div>

    <div class="yui3-g">
      <div class="yui3-u-2-3">
        <div id="visuel">
          <img src="/assets/podium/palmares.jpg" usemap="#palmares" alt="Cliquez sur son logo pour accéder au site partenaire.">
          <map name="palmares">
            <area coords="30,30,203,160"   href="#" alt="Grand Prix du Jury"        id="video1">
            <!-- <area coords="192,107,433,237" href="#" alt="Prix décerné par la CAPEB" id="video2"> -->
            <area coords="422,183,595,313" href="#" alt="Prix du public"            id="video3">
          </map>
		</div>
      </div>
      <div class="yui3-u-1-3">
        <div id="contentEast">
          <%# if eziser(323, "podium/contentEast", "png", "podium/contentEast", "jpg") %>
            <%= image_tag("/assets/podium/contentEast.jpg") %>
          <%# end %>
        </div>
      </div>
    </div>

    <div class="yui3-g">
      <div class="yui3-u-2-3">
        <div id="steps">
          <%# ezipets("palmarès",         "podium/step1") ? link_to(image_tag("podium/step1.png"), root_url, id: "step1") : nil %>
          <%# ezipets("infos\npratiques", "podium/step2") ? link_to(image_tag("podium/step2.png"), root_url, id: "step2") : nil %>
          <%= ezipets("grand prix\ndu jury",     "podium/oediv1") ? link_to(image_tag("podium/oediv1.png"), root_url, id: "oediv1") : nil %>
          <%= ezipets("prix spécial\ndu public", "podium/oediv3") ? link_to(image_tag("podium/oediv3.png"), root_url, id: "oediv3") : nil %>
          <%= ezipets("bonus",     "podium/oediv2") ? link_to(image_tag("podium/oediv2.png"), root_url, id: "oediv2") : nil %>
          <%# ezipets("prix",             "podium/step3") ? link_to(image_tag("podium/step3.png"), root_url, id: "step3") : nil %>
          <%# ezipets("villermé",         "podium/step4") ? link_to(image_tag("podium/step4.png"), root_url, id: "step4") : nil %>
          <%# ezipets("partenaires",      "podium/step5") ? link_to(image_tag("podium/step5.png"), root_url, id: "step5") : nil %>
        </div>
      </div>

<!-- id="clap" -->
      <div class="yui3-u-1-3"></div>
    </div>

    <div id="contentSouth">Avec l’aimable autorisation de Dalkia, CCMSA et la FNBTP.</div>
  </div><!-- // fin de content -->

